    <license type="expression">MIT</license>
    <requireLicenseAcceptance>true</requireLicenseAcceptance>
    <copyright>Copyright (c) 2020 TehGM</copyright>
    <dependencies>
        <group>
            <dependency id="Wolfringo.Core" version="$version$" />
            <dependency id="Wolfringo.Utilities" version="$version$" />
            <dependency id="Wolfringo.Hosting" version="$version$" />
        </group>
    </dependencies>
  </metadata>
</package>